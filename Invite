    async def zcmd(self, event):
        """ะะพะดัะปั ะดะปั ะฐะฒัะพะธะฝะฒะฐะนัะฐ ะฒ ัะฐัะฐั (ะฒะฐะนั)"""
        await event.delete()
        global text
        text = """
ะัะธะฒะตั ัะพะปะฝัะต๐
ะขะตะฑะต ะปะตัะธั ะฟัะธะณะปะฐัะตะฝะธะต ะฒ ะฟะพััััะฐััะธะน ัะฐั ั ะผะฐัะธะตะนโ๏ธ
   ะะดะตัั ะตััั ะผะฝะพะณะพ ะฝะพะฒัั ัะพะปะตะน๐งโโ๏ธ(ะะตะฒะบะฐ, ะะธะปะปะตั, ะฅะฐะบะตั ะธ ั.ะด)
   ะขั ัะพัะฝะพ ะฝะต ะฟะพะถะฐะปะตะตัั๐ตโโ๏ธ
ะะธัะธ ยซ+ยป ะธ ัััะปะบั ะฝะฐ ะฝะฐั ะธะทัะผะธัะตะปัะฝัะน ัะฐั ัั ะฟะพะปััะธัั๐พ
                """
        args = utils.get_args(event)
        chat = await event.get_input_chat()

        if args:
            count = int(args[0].strip())
        else:
            count = 100

        async for x in event.client.iter_participants(chat, limit=count):
            await event.client.send_message(int(x.id), text)
